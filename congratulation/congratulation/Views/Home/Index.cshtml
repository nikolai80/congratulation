@using congratulation.ViewModels
@model List<UserViewModel>
@{
  ViewBag.Title = "Home Page";
}

<div class="row">
  <div class="col-md-4">
    @if (Model.Count > 0)
      {
        foreach (var item in Model)
        {
        <div>@(String.IsNullOrEmpty(item.Name) == true ? "none" : item.Name)</div>
      }
    }
    else
    {<p>Список не содержит данных</p> }
  </div>

</div>